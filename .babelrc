{
	"presets": [
		"react",
		[
			"env",
			{
				"targets": {
					"browsers": "last 2 versions"
				},
				"loose": true,
				"modules": false
			}
		]
	],
	"plugins": ["transform-decorators-legacy", "react-hot-loader/babel", "transform-class-properties"],
	"env": {
		"test": {
			"plugins": ["babel-plugin-transform-es2015-modules-commonjs"]
		}
	}
}
